<p id="notice"><%= notice %></p>
<div class="btn"><%= link_to 'New Bug', new_bug_path, remote: true %></div>
<h1>Listing Bugs</h1>
<table >
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Description</th>
      <th>Issue type</th>
      <th>Priority</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id = "bugs">

    <%= render @bugs %>

  </tbody>
  
</table>

<div id="bug-form" style="display:none;"></div>
<br>


